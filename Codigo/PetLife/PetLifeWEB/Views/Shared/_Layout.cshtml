<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PetLife</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PetLifeWEB.styles.css" asp-append-version="true" />
</head>
<body>
    
    <header class="p-3 mb-3 border-bottom">
        <div class="container">

            <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 link-body-emphasis text-decoration-none">
                    <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap" /></svg>
                </a>
                
                <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                    <partial name="_LoginPartial" />
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark " asp-area="" asp-controller="Medicamento" asp-action="Index">Medicamento</a>
                    </li>
                    <li class="nav-item">
                        
                        <a class="nav-link text-dark " asp-area="" asp-controller="Pessoa" asp-action="Index" >Pessoa</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark " asp-area="" asp-controller="Pet" asp-action="Index">Pet</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark " asp-area="" asp-controller="Vacina" asp-action="Index">Vacina</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark " asp-area="" asp-controller="Vendum" asp-action="Index">Venda</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark " asp-area="" asp-controller="Petshop" asp-action="Index">Petshop</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark " asp-area="" asp-controller="Produto" asp-action="Index">Produto</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark px-2 link-body-emphasis" asp-area="" asp-controller="Consultum" asp-action="Index">Consulta</a>
                    </li>
                </ul>

                
            </div>
        </div>
    </header>
    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - PetLifeWEB - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
